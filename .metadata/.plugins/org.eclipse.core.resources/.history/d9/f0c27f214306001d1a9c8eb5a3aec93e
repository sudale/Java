package 조건문;

import java.util.Scanner;

public class If문 {

	public static void main(String[] args) {

		Scanner scanner = new Scanner(System.in);
		scanner.nextInt();
		scanner.close();
		
		int apples = 10;
		int bananas = 5;

		if (apples > bananas) {
			System.out.println("사과가 바나나보다 많음");
		}
		System.out.println("프로그램 종료");

	}

}
