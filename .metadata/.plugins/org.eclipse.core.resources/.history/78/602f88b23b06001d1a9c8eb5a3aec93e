package application;

public class 증감연산자 {

	public static void main(String[] args) {

		int count = 0;

		count = count + 1;
		System.out.println(count);
		count += 1;
		System.out.println(count);
		count++;
		System.out.println(count);
		++count;
		System.out.println(count);

		int count1 = 0;

		count1 = count1 + 1;
		System.out.println(count);
		count1 -= 1;
		System.out.println(count);
		count1--;
		System.out.println(count);
		--count1;
		System.out.println(count);

	}

}
